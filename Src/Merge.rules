<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Custom Build Rules"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="imp"
			CommandLine="lib /MACHINE:$(TargetMachine) /DEF:&quot;$(InputPath)&quot; /OUT:&quot;$(IntDir)\$(InputName).lib&quot;"
			Outputs="&quot;$(IntDir)\$(InputName).lib&quot;"
			FileExtensions="*.imp"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="po"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\Languages\$(InputFileName)&quot;"
			Outputs="&quot;$(OutDir)\Languages\$(InputFileName)&quot;"
			FileExtensions="*.po"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="flt"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\Filters\$(InputFileName)&quot;"
			Outputs="&quot;$(OutDir)\Filters\$(InputFileName)&quot;"
			FileExtensions="*.flt;*.tmpl"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="PrintTemplates"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\PrintTemplates\$(InputFileName)&quot;"
			Outputs="&quot;$(OutDir)\PrintTemplates\$(InputFileName)&quot;"
			FileExtensions="*.html"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="FileTransforms"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\FileTransforms\$(InputFileName)&quot;"
			Outputs="&quot;$(OutDir)\FileTransforms\$(InputFileName)&quot;"
			FileExtensions="*.wsc;*.xslt"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="PluginMonikers"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\PluginMonikers\$(InputFileName)&quot;"
			Outputs="&quot;$(OutDir)\PluginMonikers\$(InputFileName)&quot;"
			FileExtensions="*.ini"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="EditorScript"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\EditorScripts\$(InputName)&quot;"
			Outputs="&quot;$(OutDir)\EditorScripts\$(InputName)&quot;"
			FileExtensions="*.EditorScript"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="docs"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\Docs\$(InputFileName)&quot;"
			Outputs="&quot;$(OutDir)\Docs\$(InputFileName)&quot;"
			FileExtensions="*.chm"
			>
		</CustomBuildRule>
		<CustomBuildRule
			Name="setup"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(OutDir)\$(InputFileName)&quot;"
			Outputs="&quot;$(OutDir)\$(InputFileName)&quot;"
			FileExtensions="*.wsf;*.WinMerge"
			>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
